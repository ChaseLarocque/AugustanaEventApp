<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.augappprototype.MainActivity"
    android:background="@drawable/calendarbackground">


    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/sunday"
        app:layout_constraintGuide_begin="109dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="109dp" />

    <android.support.constraint.Guideline
        android:id="@+id/monday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="56dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="56dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tuesday"
        app:layout_constraintGuide_begin="328dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="328dp" />

    <android.support.constraint.Guideline
        android:id="@+id/wednesday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="273dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="273dp" />

    <android.support.constraint.Guideline
        android:id="@+id/thursday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="219dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="219dp" />

    <android.support.constraint.Guideline
        android:id="@+id/friday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="164dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="164dp" />
</android.support.constraint.ConstraintLayout>
